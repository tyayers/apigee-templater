{
  "name": "testproxy",
  "profile": "default",
  "proxyEndpoints": [
    {
      "name": "default",
      "basePath": "/httpbin",
      "targetName": "default",
      "targetUrl": "https://httpbin.org",
      "quotas": [
        {
          "count": "200",
          "timeUnit": "day"
        }
      ],
      "spikeArrest": {
        "rate": "20s"
